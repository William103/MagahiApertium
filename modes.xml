<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="mag-twol" install="yes">
    <pipeline>
      <program name="hfst-strings2fst -S"/>
      <program name="hfst-compose-intersect">
        <file name=".deps/mag.twol.hfst"/>
      </program>
      <program name="hfst-fst2strings"/>
    </pipeline>
  </mode>

  <mode name="mag-morph" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mag.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mag-lexc" install="yes">
    <pipeline>
      <program name="hfst-lookup">
        <file name=".deps/mag.LR.lexc.hfst"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mag-gener" install="yes">
    <pipeline>
      <program name="lt-proc -g">
        <file name="mag.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mag-tagger" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mag.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="mag.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mag-disam" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mag.automorf.bin"/>
      </program>
      <program name="cg-conv -a"/>
      <program name="vislcg3 --trace --grammar">
        <file name="mag.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>

